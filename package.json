    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "gulp": "^3.8.11",
    "gulp-sass": "^2.0.1",
    "gulp-sourcemaps": "^1.5.2",
    "jquery": "^3.1.0",
    "lodash": "^4.15.0",
    "node-sass": "^4.5.3"
  }
